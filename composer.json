{
    "name": "tenolo/open-graph-bundle",
    "description": "A Symfony OpenGraph bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "opengraph",
        "open",
        "graph",
        "og",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "authors": [
        {
            "name": "tenolo GmbH & Co. KG",
            "email": "info@tenolo.de",
            "homepage": "https://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        },
        {
            "name": "Manuel Dalla Lana",
            "email": "manuel@pontedilana.it",
            "homepage": "https://www.pontedilana.it"
        }
    ],
    "homepage": "https://github.com/tenolo/open-graph-bundle",
    "support": {
        "email": "info@tenolo.de",
        "issues": "https://github.com/tenolo/open-graph-bundle/issues",
        "wiki": "https://github.com/tenolo/open-graph-bundle/wiki",
        "source": "https://github.com/tenolo/open-graph-bundle/releases"
    },
    "require": {
        "php": "7.4.* || 8.1.* || 8.2.*",
        "euskadi31/opengraph": "^1.0 || ^2.0",
        "symfony/config": "^5.4 || ^6.2",
        "symfony/dependency-injection": "^5.4 || ^6.2",
        "symfony/framework-bundle": "^5.4 || ^6.2",
        "symfony/http-kernel": "^5.4 || ^6.2",
        "symfony/twig-bundle": "^5.4 || ^6.2",
        "twig/twig": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.4 || ^6.2",
        "matthiasnoback/symfony-config-test": "^4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\OpenGraphBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\OpenGraphBundle\\Tests\\": "tests/"
        }
    }
}
